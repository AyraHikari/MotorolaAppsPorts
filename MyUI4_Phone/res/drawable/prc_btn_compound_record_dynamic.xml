<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_checked="true" android:state_pressed="false">
        <bitmap android:src="@drawable/prc_ic_unrecord_normal" android:tint="@color/prc_callbutton_text_record_checked_normal_color" android:tintMode="src_in" />
    </item>
    <item android:state_enabled="true" android:state_checked="true" android:state_pressed="true">
        <bitmap android:src="@drawable/prc_ic_unrecord_normal" android:tint="@color/prc_callbutton_text_record_checked_normal_color" android:alpha="0.4" android:tintMode="src_in" />
    </item>
    <item android:state_enabled="true" android:state_checked="false" android:state_pressed="false">
        <bitmap android:src="@drawable/prc_ic_unrecord_normal" />
    </item>
    <item android:state_enabled="true" android:state_checked="false" android:state_pressed="true">
        <bitmap android:src="@drawable/prc_ic_unrecord_normal" android:alpha="0.4" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/prc_ic_unrecord_normal" android:alpha="0.4" />
    </item>
</selector>